<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.prokosna.springddd.domain.repository.store.BookFooRepository">
    <select id="findById" resultType="xyz.prokosna.springddd.domain.model.store.Book">
        SELECT *
        FROM books
        WHERE id = #{id}
    </select>
</mapper>
